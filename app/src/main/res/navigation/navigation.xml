<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.driveawayz.dashboard.homeFrag.PickUpPoint"
        android:label="Home"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/nav_notifications"
        android:name="com.driveawayz.dashboard.notificationFrag.NotificationFragment"
        android:label="Notifications"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/nav_mydrive"
        android:name="com.driveawayz.dashboard.mydriveFrag.MyDrivesFragment"
        android:label="MyDrives"
        tools:layout="@layout/fragment_my_drives" />

    <fragment
        android:id="@+id/nav_paymentsmethods"
        android:name="com.driveawayz.dashboard.paymentFrag.PaymentFragment"
        android:label="Payment Methods"
        tools:layout="@layout/fragment_payment" />

    <fragment
        android:id="@+id/nav_setting"
        android:name="com.driveawayz.dashboard.setiingFrag.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/nav_support"
        android:name="com.driveawayz.dashboard.supportFrag.SupportFragment"
        android:label="Support"
        tools:layout="@layout/fragment_support" />

</navigation>